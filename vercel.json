{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "public/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "public/sitemap.xml"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "VIEW_COMPILED_PATH": "/tmp/views"
  },
  "outputDirectory": "public/build",
  "buildCommand": "npm run build"
}